{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "~5.0",
        "illuminate/html": "~5.0",
        "filp/whoops": "~1.0",
        "patchwork/utf8": "~1.2",
        "laravel/elixir": "~1.0",
        "laracasts/commander": "dev-master",
        "laracasts/flash": "~1.0",
        "laracasts/presenter": "0.2.*",
        "guzzlehttp/guzzle": "~5.0",
        "mcamara/laravel-localization": "dev-Laravel5Support",
        "dimsav/laravel-translatable": "dev-development",
        "intervention/image": "~2.0",
		"cartalyst/sentry": "2.1.*",
        "baum/baum": "~1.0"
	},
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.dogstudio.be"
        },
        {
            "type": "vcs",
            "url": "https://github.com/julienroland/laravel-localization"
        }
    ],
	"require-dev": {
		"phpunit/phpunit": "~4.0"
	},
	"autoload": {
		"classmap": [
			"database",
			"tests/TestCase.php"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan route:scan",
			"php artisan event:scan",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
